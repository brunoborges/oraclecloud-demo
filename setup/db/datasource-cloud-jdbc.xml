<jdbc-data-source xmlns="http://xmlns.oracle.com/weblogic/jdbc-data-source">
	<name>jdbc/OE</name>
	<jdbc-driver-params>
		<!-- verify the URL below before deploying -->
		<url>jdbc:oracle:thin:@//YOUR-DBCS-INSTANCE-HOSTNAME:1521/DEMOPDB.your-identity-domain.oraclecloud.internal</url>
		<driver-name>oracle.jdbc.xa.client.OracleXADataSource</driver-name>
		<properties>
			<property>
				<name>user</name>
				<value>oe</value>
			</property>
			<property>
				<name>password</name>
				<value>oe</value>
			</property>
		</properties>
	</jdbc-driver-params>
	<jdbc-data-source-params>
		<jndi-name>jdbc/OE</jndi-name>
	</jdbc-data-source-params>
</jdbc-data-source>
